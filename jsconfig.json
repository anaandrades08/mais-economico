{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@styles/*": ["./app/styles/*"],
      "@data/*": ["./app/data/*"],
      "@/*": ["./*"],
      "@/app/*": ["./app/*"],
      "@/lib/*": ["./app/lib/*"],
      "@/api/*": ["./app/api*"],

    }
  },
  "include": ["**/*.ts", "**/*.tsx", "**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
